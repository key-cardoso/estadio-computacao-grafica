#VRML V2.0 utf8
#base geral
Transform {
	translation 0 -2.2 0
	scale -1.5 0.1 1
	
	children [
Shape {
	appearance Appearance {
		material Material {
			diffuseColor 33 34 6
			specularColor 29 29 29
			ambientIntensity 33
			shininess 79
			
		}
	}
	geometry Extrusion {
		scale 12 12
		solid FALSE
	
	}

}
	]
}
#campo de futebol
Transform {
	translation 0 -2 0
	scale -1.5 0.1 1
	
	children [
Shape {
	appearance Appearance {
		texture	ImageTexture {
			url	["base.jpg"]
		}
	}
	geometry Box {
		size 8 0.1 5
	
	}

}
	]
}

